                "context": {
                    "type": "string"
                },
                "fileID": {
                    "type": "integer"
                },
                "invID": {
                    "type": "string"
                },
                "invertBit": {
                    "type": "string"
                },
                "messageClass": {
                    "type": "string"
                },
                "messageID": {
                    "type": "string"
                },
                "messageLevel": {
                    "type": "string"
                },
                "messageText": {
                    "type": "string"
                },
                "messageType": {
                    "type": "string"
                },
                "mqtt": {
                    "type": "string"
                },
                "number": {
                    "type": "string"
                },
                "unitGUID": {
                    "type": "string"
                },
                "varAddress": {
                    "type": "string"
                }
